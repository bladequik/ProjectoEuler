def soma_dos_quadrados(n):
    resposta = 0
    for i in range(n+1):
        resposta += pow(i,2)
    return resposta

def soma_n_int_consecutivos(m):
    resposta = 0
    for j in range(m+1):
        resposta += j
    return pow(resposta,2)
    
print (soma_n_int_consecutivos(100) - soma_dos_quadrados(100))

